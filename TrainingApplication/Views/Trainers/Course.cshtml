@model IEnumerable<TrainingApplication.Models.TraineesCourse>
@{
    ViewBag.Title = "Course";
}

<h2>Your Courses and Trainees</h2>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Course</th>
            <th scope="col">Trainees</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.Course.Name</td>
                <td>@item.Trainee.Email</td>
            </tr>
        }
    </tbody>
</table>
